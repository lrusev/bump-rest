{
    "name": "bump/bump-rest",
    "version": "0.0.1",
    "type": "library",
    "description": "This library contain some helpful classes to use with FosRestBundle",
    "keywords": ["bumpnetworks", "rest", "api"],
    "license": "MIT",
    "repositories": [{
        "type": "package",
        "package": {
            "name": "lrusev/JsonPath",
            "version": "3.0.0",
            "source": {
                "url": "https://github.com/lrusev/JsonPath",
                "type": "git",
                "reference": "releases/tag/3.0.0"
            }
        }
    }],
    "require": {
        "php": ">=5.3.0",
        "jms/serializer-bundle": "~0.13.0",
        "friendsofsymfony/rest-bundle": "1.3.1",
        "nelmio/api-doc-bundle": "~2.4",
        "willdurand/hateoas-bundle": "0.2.*",
        "hautelook/templated-uri-bundle": "~1.0",
        "willdurand/rest-extra-bundle": "~1.0",
        "peekmo/jsonpath": "@dev"
    },
    "require-dev": {
        "liip/functional-test-bundle": "dev-master",
        "phpunit/phpunit": "*"
    },
    "authors": [{
        "name": "Leonid Rusev",
        "email": "lgrusev@od.bumpnetworks.com"
    }],
    "autoload": {
        "psr-0": {
            "Bump": ""
        }
    }
}
